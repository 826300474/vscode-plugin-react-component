{
	"name": "react-component",
	"displayName": "react-component",
	"description": "自动创建react组件所需要的文件目录",
	"publisher": "826300474",
	"version": "0.0.6",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:react-component.creat"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "react-component.creat",
				"title": "新建React组件"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "react-component.creat",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
